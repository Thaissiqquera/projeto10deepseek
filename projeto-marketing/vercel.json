{
  "version": 2,
  "builds": [
    {
      "src": "api/analysis.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/",
      "dest": "/templates/index.html"
    },
    {
      "src": "/api/analysis",
      "dest": "/api/analysis.py"
    }
  ]
}